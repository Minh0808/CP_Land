{
      "version": 2,
      "builds": [
      {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist" }
      },
      {
            "src": "Server/server.ts",
            "use": "@vercel/node"
      }
      ],
      "routes": [
      {
         "src": "/api/(.*)$",
         "dest": "/Server/server.ts"
      },
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ]
 }
 