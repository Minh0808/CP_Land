
{
   "version": 2,
   "builds": [
      {
         "src": "Server/server.ts",
         "use": "@vercel/node"
      }
      ],
      "routes": [

      {
         "src": "/api/(.*)",
         "dest": "Server/server.ts"
      },
      {
         "src": "/$",
         "dest": "Server/server.ts"
      }
   ]
}
 