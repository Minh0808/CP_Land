{
   "version": 2,
   "builds": [
      {
         "src": "Server/server.ts",
         "use": "@vercel/node"
      },
      {
         "src": "package.json",
         "use": "@vercel/static-build",
         "config": { "distDir": "dist" }
      }
      ],
      "routes": [
      { "src": "/api/(.*)", "dest": "Server/server.ts" }
   ]
}
 